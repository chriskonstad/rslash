{
  "name": "reddit.com/r/",
  "description" : "Quickly jump to subreddit by typing 'r' into the omnibar",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "omnibox": { "keyword" : "r" },
  "manifest_version": 2,
  "permissions": [
    "storage",
    "tabs",
    "webRequest",
    "*://*.reddit.com/"
  ],
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}
